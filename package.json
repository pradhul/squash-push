{
  "name": "squash-push",
  "displayName": "squash&push",
  "publisher": "PradhulDev",
  "icon": "icon.png",
  "description": "squash the commits in local to be able to push it as a single commit",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "repository": {
  "type": "git",
  "url": "https://github.com/pradhul/squash-push"
},
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "squash-push.squashCommits",
        "title": "Squash Local Commits"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
